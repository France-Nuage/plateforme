{
  "name": "@france-nuage/types",
  "version": "0.0.0",
  "private": "true",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "tsc --watch",
    "build": "tsup src/index.ts --format cjs,esm --dts"
