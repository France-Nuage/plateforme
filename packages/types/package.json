      "import": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@france-nuage/typescript-config": "*",
    "tsup": "^7.2.0",
    "typescript": "^5.1"
  }
}
